[package]
name = "sparky_sentinel"
version = "0.1.0"
description = "Created with Anchor"
edition = "2021"

[lib]
crate-type = ["cdylib", "lib"]
name = "sparky_sentinel"

[features]
no-entrypoint = []
no-idl = []
no-log-ix-name = []
cpi = ["no-entrypoint"]
default = []

[dependencies]
anchor-lang = "0.29.0"
solana-program = "=1.17.0"
borsh = "0.10.3"

[patch.crates-io]
constant_time_eq = { git = "https://github.com/cesarghali/constant_time_eq", tag = "v0.3.0" }
